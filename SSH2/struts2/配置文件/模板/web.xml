<?xml version="1.0" encoding="utf-8"?>
<web-app>
    <filter>
        <filter-name>testFilter</filter-name>
        <filter-class>
        	<!--struts2.5 版本没有 ng--> 
            org.apache.struts2.dispatcher.ng.filter.StrutsPrepareAndExecuteFilter
        </filter-class>
    </filter>
    <filter-mapping>
        <filter-name>testFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <welcome-file-list>
        <welcome-file>/start.jsp</welcome-file>
    </welcome-file-list>
</web-app>
